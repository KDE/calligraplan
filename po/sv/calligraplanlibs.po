msgstr "Schemalagd starttid"

#: models/kptcommonstrings.cpp:143
#, kde-kuit-format
msgctxt "@info:tooltip"
msgid "The scheduled finish time"
msgstr "Schemalagd sluttid"

#: models/kptcommonstrings.cpp:144
